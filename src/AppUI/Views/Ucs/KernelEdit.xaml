<UserControl x:Class="NTMiner.Views.Ucs.KernelEdit"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
		xmlns:controls="clr-namespace:NTMiner.Controls;assembly=NTMinerWpf"
		xmlns:vm="clr-namespace:NTMiner.Vms"
		mc:Ignorable="d"
		d:DataContext="{d:DesignData Source=Design/KernelViewModel.xaml}"
        Background="White"
		d:DesignWidth="660" d:DesignHeight="560">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/AppUI;component/Styles/KbCoinCode.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="LblTb" TargetType="TextBlock">
                <Setter Property="FontSize" Value="12"></Setter>
                <Setter Property="Padding" Value="0 0 2 0"></Setter>
                <Setter Property="Width" Value="90"></Setter>
                <Setter Property="VerticalAlignment" Value="Center"></Setter>
                <Setter Property="TextAlignment" Value="Right"></Setter>
                <Setter Property="TextWrapping" Value="Wrap"></Setter>
            </Style>
            <sys:String x:Key="WindowTitle">
                WindowTitle
            </sys:String>
            <sys:String x:Key="Kernel">
                Kernel
            </sys:String>
            <sys:String x:Key="Code">
                Code
            </sys:String>
            <sys:String x:Key="Version">
                Version
            </sys:String>
            <sys:String x:Key="Package">
                Package
            </sys:String>
            <sys:String x:Key="Browser">
                Browser
            </sys:String>
            <sys:String x:Key="PackageHistory">
                PackageHistory
            </sys:String>
            <sys:String x:Key="PackageHistoryTip">
                PackageHistoryTip
            </sys:String>
            <sys:String x:Key="Notice">
                Notice
            </sys:String>
            <sys:String x:Key="Help">
                Help
            </sys:String>
            <sys:String x:Key="KernelOutput">
                KernelOutput
            </sys:String>
            <sys:String x:Key="Args">
                Args
            </sys:String>
            <sys:String x:Key="IsSupportDualMine">
                IsSupportDualMine
            </sys:String>
            <sys:String x:Key="DualWeightArg">
                DualWeightArg
            </sys:String>
            <sys:String x:Key="IsAutoDualWeight">
                IsAutoDualWeight
            </sys:String>
            <sys:String x:Key="DualWeightMin">
                DualWeightMin
            </sys:String>
            <sys:String x:Key="DualWeightMax">
                DualWeightMax
            </sys:String>
            <sys:String x:Key="DualCoinGroup">
                DualCoinGroup
            </sys:String>
            <sys:String x:Key="DualCoinGroupDescription">
                DualCoinGroupDescription
            </sys:String>
            <sys:String x:Key="DualFullArgs">
                DualFullArgs
            </sys:String>
            <sys:String x:Key="Coin">
                Coin
            </sys:String>
            <sys:String x:Key="Add">
                Add
            </sys:String>
            <sys:String x:Key="SupportedGpus">
                SupportedGpus
            </sys:String>
            <sys:String x:Key="CoinKernelArgs">
                CoinKernelArgs
            </sys:String>
            <sys:String x:Key="TbNoRecord">
                TbNoRecord
            </sys:String>
            <sys:String x:Key="Manage">
                Manage
            </sys:String>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <TabControl 
		    Padding="0" BorderThickness="0" 
		    BorderBrush="{StaticResource LineColor}">
            <controls:KbTabItem>
                <controls:KbTabItem.Header>
                    <TextBlock Text="{DynamicResource Kernel}" Padding="8 0"></TextBlock>
                </controls:KbTabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="560"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <StackPanel Margin="0 0 10 10">
                        <WrapPanel Margin="0 2">
                            <TextBlock Style="{StaticResource LblTb}" Text="{DynamicResource Code}"></TextBlock>
                            <TextBox 
                                Height="26"
						        Width="180"
						        IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						        VerticalContentAlignment="Center" Text="{Binding Code, ValidatesOnExceptions=True}">
                            </TextBox>
                            <TextBlock Style="{StaticResource LblTb}" Text="{DynamicResource Version}"></TextBlock>
                            <TextBox 
						        Width="180"
						        IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						        VerticalContentAlignment="Center" Text="{Binding Version, ValidatesOnExceptions=True}">
                            </TextBox>
                        </WrapPanel>

                        <WrapPanel Margin="0 2">
                            <TextBlock Style="{StaticResource LblTb}" Text="{DynamicResource Package}"></TextBlock>
                            <Grid Width="430">
                                <WrapPanel VerticalAlignment="Center">
                                    <TextBox IsReadOnly="True" BorderThickness="0" VerticalAlignment="Center" Text="{Binding Package, Mode=OneWay}">
                                    </TextBox>
                                    <Button 
						                Margin="4 0 0 0"
                                        Padding="4 0"
						                IsEnabled="{x:Static vm:AppStatic.IsDevMode}"
						                VerticalAlignment="Center"
						                Command="{Binding BrowsePackage}"
						                Content="{DynamicResource Browser}">
                                    </Button>
                                    <TextBlock Margin="20 0 0 0" VerticalAlignment="Center" Text="{Binding SizeMbText}">
                                    </TextBlock>
                                </WrapPanel>
                            </Grid>
                        </WrapPanel>

                        <WrapPanel Margin="0 2" Visibility="{x:Static vm:AppStatic.IsDevModeVisible}">
                            <TextBlock Style="{StaticResource LblTb}" Text="{DynamicResource PackageHistory}"></TextBlock>
                            <TextBox 
                                ToolTip="{DynamicResource PackageHistoryTip}"
						        Width="450" Background="Transparent" Padding="0 4"
						        Text="{Binding PackageHistory, Mode=TwoWay}">
                            </TextBox>
                        </WrapPanel>

                        <WrapPanel Margin="0 2">
                            <TextBlock Style="{StaticResource LblTb}" VerticalAlignment="Top" Text="{DynamicResource Notice}"></TextBlock>
                            <TextBox 
						        Width="450" Background="Transparent" Padding="0 4"
						        IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						        AcceptsReturn="True" TextWrapping="Wrap" 
						        VerticalContentAlignment="Top" VerticalScrollBarVisibility="Auto"
						        Text="{Binding Notice, Mode=TwoWay}" Height="50">
                            </TextBox>
                        </WrapPanel>

                        <WrapPanel Margin="0 2">
                            <TextBlock Style="{StaticResource LblTb}">
                                <WrapPanel>
                                    <TextBlock Text="{DynamicResource Help}"></TextBlock>
                                    <controls:KbLinkButton
                                        Margin="4 0 0 0"
							            Visibility="{Binding HelpArg, Converter={StaticResource NotNullOrEmptyVisibilityConverter}}"
							            HorizontalAlignment="Right"
							            ToolTip="{DynamicResource Help}"
							            KbDisplayLine="False"
							            KbSystemButtonHoverBackground="Transparent" 
							            Background="Transparent"
							            Command="{Binding ShowKernelHelp}">
                                        <WrapPanel>
                                            <Path
									            Width="14"
									            Height="14"
									            Data="{StaticResource Icon_Help}"
									            Fill="{StaticResource IconFillColor}"
									            Stretch="Fill" />
                                        </WrapPanel>
                                    </controls:KbLinkButton>
                                </WrapPanel>
                            </TextBlock>
                            <TextBox 
                                Height="26"
						        Width="180"
						        IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						        VerticalContentAlignment="Center" Text="{Binding HelpArg}">
                            </TextBox>
                            <TextBlock Style="{StaticResource LblTb}" Text="{DynamicResource KernelOutput}"></TextBlock>
                            <controls:KbComboBox 
                                BorderThickness="0"
				                IsEnabled="{x:Static vm:AppStatic.IsDevMode}"
						        Width="180" ItemsSource="{Binding KernelOutputVms.PleaseSelectVms}"
                                DisplayMemberPath="Name" SelectedItem="{Binding KernelOutputVm}">
                                <controls:KbComboBox.Toolbar>
                                    <WrapPanel HorizontalAlignment="Right">
                                        <controls:KbButton
                                                Margin="4 2"
                                                BorderThickness="0"
                                                Height="24"
                                                Padding="4 0"
                                                Background="{StaticResource IconFillColor}"
                                                Command="{x:Static vm:AppStatic.ShowKernelOutputs}"
                                                CornerRadius="2">
                                            <WrapPanel>
                                                <Path
                                                        Width="14"
                                                        Height="14"
                                                        Data="{StaticResource Icon_KernelOutput}"
                                                        Fill="White"
                                                        Stretch="Fill" />
                                                <TextBlock Margin="4 0 0 0" Text="{DynamicResource Manage}" Foreground="White"></TextBlock>
                                            </WrapPanel>
                                        </controls:KbButton>
                                    </WrapPanel>
                                </controls:KbComboBox.Toolbar>
                            </controls:KbComboBox>
                        </WrapPanel>

                        <WrapPanel Margin="0 2">
                            <TextBlock Style="{StaticResource LblTb}" VerticalAlignment="Top" Text="{DynamicResource Args}"></TextBlock>
                            <TextBox 
						        Width="450" Background="Transparent" Padding="0 4"
						        IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						        AcceptsReturn="True" TextWrapping="Wrap" 
						        VerticalContentAlignment="Top" VerticalScrollBarVisibility="Auto"
						        Text="{Binding Args, Mode=TwoWay}" Height="60">
                            </TextBox>
                        </WrapPanel>

                        <WrapPanel Margin="0 2" Visibility="{Binding IsSupportDualMineVisible}">
                            <TextBlock Style="{StaticResource LblTb}"></TextBlock>
                            <controls:KbCheckBox 
                                Height="26"
				                IsEnabled="{x:Static vm:AppStatic.IsDevMode}"
				                Content="{DynamicResource IsSupportDualMine}"
				                IconColor="{StaticResource IconFillColor}"
				                IsChecked="{Binding IsSupportDualMine}" />
                            <WrapPanel 
                                Margin="10 0 0 0" 
                                Visibility="{Binding IsSupportDualMine, Converter={StaticResource BoolToVisibilityConverter}}">
                                <TextBlock Style="{StaticResource LblTb}" Width="Auto" Text="{DynamicResource DualWeightArg}"></TextBlock>
                                <TextBox 
                                    Height="26"
						            Width="120"
						            IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						            VerticalContentAlignment="Center" Text="{Binding DualWeightArg}">
                                </TextBox>
                            </WrapPanel>
                        </WrapPanel>

                        <StackPanel Visibility="{Binding IsSupportDualMine, Converter={StaticResource BoolToVisibilityConverter}}">
                            <WrapPanel Margin="0 2">
                                <TextBlock Style="{StaticResource LblTb}"></TextBlock>
                                <controls:KbCheckBox 
				                    IsEnabled="{x:Static vm:AppStatic.IsDevMode}"
				                    Content="{DynamicResource IsAutoDualWeight}"
				                    IconColor="{StaticResource IconFillColor}"
				                    IsChecked="{Binding IsAutoDualWeight}" />
                                <TextBlock Margin="8 0 0 0" Text="{DynamicResource DualWeightMin}" VerticalAlignment="Center"></TextBlock>
                                <TextBox 
                                    Height="26"
						            Width="50"
						            IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						            VerticalContentAlignment="Center" Text="{Binding DualWeightMin}">
                                </TextBox>
                                <TextBlock Margin="8 0 0 0" Text="{DynamicResource DualWeightMax}" VerticalAlignment="Center"></TextBlock>
                                <TextBox 
                                    Height="26"
						            Width="50"
						            IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
						            VerticalContentAlignment="Center" Text="{Binding DualWeightMax}">
                                </TextBox>
                            </WrapPanel>

                            <WrapPanel Margin="0 2">
                                <TextBlock Style="{StaticResource LblTb}" Text="{DynamicResource DualCoinGroup}" ToolTip="{DynamicResource DualCoinGroup}"></TextBlock>
                                <controls:KbComboBox 
                                    BorderThickness="0"
				                    IsEnabled="{x:Static vm:AppStatic.IsDevMode}"
						            Width="200" ItemsSource="{Binding GroupVms.SelectionOptions}"
                                    DisplayMemberPath="Name"
                                    SelectedItem="{Binding DualCoinGroup}">
                                </controls:KbComboBox>
                                <TextBlock 
                                    Margin="2 0 0 0"
                                    Visibility="{Binding IsDualCoinGroupEmpty, Converter={StaticResource BoolToVisibilityConverter}}"
                                    VerticalAlignment="Center" Foreground="Red" Text="{DynamicResource DualCoinGroupDescription}"></TextBlock>
                            </WrapPanel>

                            <WrapPanel Margin="0 2">
                                <TextBlock Style="{StaticResource LblTb}" VerticalAlignment="Top" Text="{DynamicResource DualFullArgs}" ToolTip="{DynamicResource DualFullArgs}"></TextBlock>
                                <TextBox 
				                    Width="450" Background="Transparent"
				                    IsReadOnly="{x:Static vm:AppStatic.IsNotDevMode}"
				                    AcceptsReturn="True" TextWrapping="Wrap" 
				                    VerticalContentAlignment="Top" VerticalScrollBarVisibility="Auto"
				                    Text="{Binding DualFullArgs, Mode=TwoWay}" Height="80">
                                </TextBox>
                            </WrapPanel>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Grid.Column="1" Visibility="{x:Static vm:AppStatic.IsDevModeVisible}" HorizontalAlignment="Left">
                        <WrapPanel>
                            <TextBlock Style="{StaticResource LblTb}" Width="Auto" Text="{DynamicResource Sort}"></TextBlock>
                            <TextBox Width="40" Height="26" VerticalContentAlignment="Center" Text="{Binding SortNumber, ValidatesOnExceptions=True}">
                            </TextBox>
                        </WrapPanel>
                        <TextBox 
                            Width="100" Margin="0 6 0 0" BorderThickness="0" IsReadOnly="True" 
                            AcceptsReturn="True" TextWrapping="Wrap">mainCoin、mainAlgo、wallet、host、port、pool、worker</TextBox>
                        <TextBox 
                            Margin="0"
                            Visibility="{Binding IsSupportDualMine, Converter={StaticResource BoolToVisibilityConverter}}"
                            Width="100" BorderThickness="0" IsReadOnly="True" AcceptsReturn="True" 
                            TextWrapping="Wrap">dualCoin、dualAlgo、dualWallet、dualHost、dualPort、dualPool</TextBox>
                    </StackPanel>

                    <controls:KbButton
                        VerticalAlignment="Bottom"
                        Command="{Binding Save}" 
                        Visibility="{x:Static vm:AppStatic.IsDevModeVisible}"
                        Margin="0 10 0 0"
                        Width="70" 
                        Height="30"
                        Background="{StaticResource IconFillColor}"
                        Foreground="White"
                        CornerRadius="2">
                        <WrapPanel>
                            <Path
                                Width="12"
                                Height="12"
                                Data="{StaticResource Icon_Ok}"
                                Fill="White"
                                Stretch="Fill" />
                            <TextBlock
                                Padding="4"
                                VerticalAlignment="Center"
                                Foreground="White"
                                Text="确定" />
                        </WrapPanel>
                    </controls:KbButton>
                </Grid>
            </controls:KbTabItem>

            <!--start支持币种-->
            <controls:KbTabItem>
                <controls:KbTabItem.Header>
                    <TextBlock Padding="8 0" Text="{DynamicResource Coin}"></TextBlock>
                </controls:KbTabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="23*"/>
                        <ColumnDefinition Width="637*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <WrapPanel Grid.ColumnSpan="2" Margin="0,0,0,38">
                        <Label Width="40" Content="{DynamicResource Coin}"></Label>
                        <controls:KbComboBox 
                            x:Name="CbCoins"
                            BorderThickness="0"
						    Width="132" ItemsSource="{Binding KernelVmSingleInstance.CoinVms}" 
						    ItemTemplate="{StaticResource CoinCodeDataTemplate}">
                            <controls:KbComboBox.ItemContainerStyle>
                                <Style TargetType="ComboBoxItem">
                                    <Setter Property="Height" Value="30"></Setter>
                                    <Setter  Property="IsEnabled" Value="{Binding IsSupported}" />
                                </Style>
                            </controls:KbComboBox.ItemContainerStyle>
                        </controls:KbComboBox>
                        <controls:KbButton 
                            Command="{Binding KernelVmSingleInstance.AddCoinKernel}" 
                            CommandParameter="{Binding ElementName=CbCoins, Path=SelectedItem}"
                            Background="Transparent" BorderThickness="0"
                            Margin="2 0"
                            CornerRadius="2" 
                            Content="{DynamicResource Add}">
                        </controls:KbButton>
                    </WrapPanel>
                    <DataGrid
                        ItemsSource="{Binding KernelVmSingleInstance.CoinKernels}" 
                        BorderThickness="1 0 0 0" BorderBrush="{StaticResource LineColor}"
                        MouseDoubleClick="CoinKernelDataGrid_MouseDoubleClick" Grid.ColumnSpan="2" Margin="0,25,0,0" Grid.RowSpan="2">
                        <DataGrid.Resources>
                            <vm:BindingProxy x:Key="proxy" Data="{Binding}" />
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTemplateColumn 
                                MinWidth="100" CanUserResize="False" CanUserReorder="False"
                                CanUserSort="False" IsReadOnly="True">
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="{DynamicResource Coin}"></TextBlock>
                                </DataGridTemplateColumn.Header>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <WrapPanel>
                                            <TextBlock Text="{Binding CoinCode}"></TextBlock>
                                            <TextBlock Text="-"></TextBlock>
                                            <TextBlock Text="{Binding CoinVm.Algo}"></TextBlock>
                                        </WrapPanel>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="80" CanUserResize="False" CanUserReorder="False" CanUserSort="False" IsReadOnly="True">
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="{DynamicResource SupportedGpus}"></TextBlock>
                                </DataGridTemplateColumn.Header>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <WrapPanel>
                                            <Path
                                                Margin="4 0 0 0"
				                                Width="18"
				                                Height="14"
                                                ToolTip="NVIDIA"
                                                Visibility="{Binding IsNvidiaIconVisible}"
				                                Data="{StaticResource Icon_Nvidia}"
				                                Fill="Green"
				                                Stretch="Fill" />
                                            <Path
                                                Margin="4 0 0 0"
				                                Width="16"
				                                Height="14"
                                                ToolTip="AMD"
                                                Visibility="{Binding IsAMDIconVisible}"
				                                Data="{StaticResource Icon_AMD}"
				                                Fill="Red"
				                                Stretch="Fill" />
                                        </WrapPanel>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="160" CanUserResize="False" CanUserReorder="False" CanUserSort="False" IsReadOnly="True">
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="{DynamicResource DualCoinGroup}"></TextBlock>
                                </DataGridTemplateColumn.Header>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding DualCoinGroup.Name}"></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="*" CanUserResize="False" CanUserReorder="False" CanUserSort="False" IsReadOnly="True">
                                <DataGridTemplateColumn.Header>
                                    <TextBlock Text="{DynamicResource CoinKernelArgs}"></TextBlock>
                                </DataGridTemplateColumn.Header>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Args}"></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn 
                                MinWidth="80" CanUserResize="False" CanUserSort="False" IsReadOnly="True"
                                Visibility="{x:Static vm:AppStatic.IsDevModeVisible}">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <StackPanel HorizontalAlignment="Right" Background="Transparent" VerticalAlignment="Center" Orientation="Horizontal">
                                            <controls:KbButton Command="{Binding Edit}" Background="Transparent" BorderThickness="0"
                                                Margin="2 0"
                                                CornerRadius="2" ToolTip="编辑">
                                                <WrapPanel>
                                                    <Path
                                                        Width="18"
                                                        Height="18"
                                                        Data="{StaticResource Icon_Edit}"
                                                        Fill="{StaticResource IconFillColor}"
                                                        Stretch="Fill" />
                                                </WrapPanel>
                                            </controls:KbButton>
                                            <controls:KbButton Command="{Binding Remove}" Background="Transparent" BorderThickness="0"
                                                Margin="2 0"
                                                CornerRadius="2" ToolTip="删除">
                                                <WrapPanel>
                                                    <Path
                                                        Width="18"
                                                        Height="18"
                                                        Data="{StaticResource Icon_Delete}"
                                                        Fill="{StaticResource IconFillColor}"
                                                        Stretch="Fill" />
                                                </WrapPanel>
                                            </controls:KbButton>
                                        </StackPanel>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <TextBlock Grid.Row="1" Visibility="{Binding KernelVmSingleInstance.CoinKernels, Converter={StaticResource NoRecordConverter}}" Foreground="Red" HorizontalAlignment="Center" VerticalAlignment="Center" Text="{DynamicResource TbNoRecord}" Grid.Column="1" Margin="271,209,293,246"></TextBlock>
                </Grid>
            </controls:KbTabItem>
            <!--end支持币种-->
        </TabControl>
        <controls:KbButton
            Margin="4"
            VerticalAlignment="Top"
            HorizontalAlignment="Right"
            Visibility="{x:Static vm:AppStatic.IsDevModeVisible}"
            ToolTip="KernelEdit translate"
            Command="{x:Static vm:AppStatic.ShowLangViewItems}"
            CommandParameter="KernelEdit"
            Background="Transparent">
            <WrapPanel>
                <Path
                    Width="14"
                    Height="14"
                    Data="{StaticResource Icon_Translate}"
                    Fill="Red"
                    Stretch="Fill" />
            </WrapPanel>
        </controls:KbButton>
    </Grid>
</UserControl>
